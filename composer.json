{
    "name": "dario_swain/re-captcha-library",
    "type": "library",
    "description": "Google ReCAPTCHA (ver. 2) backend provider.",
    "keywords": ["recaptcha", "php", "library", "google api", "validator", "captcha"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Pokamestov",
            "email": "dario_swain@yahoo.com",
            "homepage": "http://github.com/DarioSwain",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
	    "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "2.5.*",
        "symfony/yaml": "2.8.*"
    },
    "autoload": {
